@model SFA.DAS.LoginService.Web.Controllers.InvitationsWeb.ViewModels.InvitationExpiredViewModel

@{
    ViewBag.Title = "Invitation expired";
}

<main class="govuk-main-wrapper " id="main-content" role="main">
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">

            <h1 class="govuk-heading-xl">Invitation expired</h1>

            <form asp-action="Reinvite" asp-route-invitationId="@Model.InvitationId" >
                <button type="submit" class="govuk-button">Request a new link</button>
            </form>

        </div>
    </div>
</main>